<Window x:Class="BIMS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BIMS"
        mc:Ignorable="d"
        Title="Load from a extend file." Height="500" Width="350">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Name="LoadFromAExtendFile" Grid.Row="0" Height="50" Margin="10,0,10,0" Content="Load from a extend file..." Click="LoadFromAExtendFile_Click">

        </Button>
        <ListBox Name="listInformation"  Grid.Row="1" Margin="10,0,10,10"  ItemsSource="{Binding ElementName=UI, Path=ListMessages}"></ListBox>
     
    </Grid>
</Window>
